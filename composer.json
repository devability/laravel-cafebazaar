{
    "name": "devability/laravel-cafebazaar",
    "description": "Cafebazaar development api package",
    "keywords": [
        "devability",
        "laravel-cafebazaar"
    ],
    "homepage": "https://github.com/devability/laravel-cafebazaar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mahmood A",
            "email": "devability2020@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/support": "^6.2"
    },
    "require-dev": {
        "orchestra/testbench": "4.x",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Devability\\LaravelCafebazaar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devability\\LaravelCafebazaar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devability\\LaravelCafebazaar\\LaravelCafebazaarServiceProvider"
            ],
            "aliases": {
                "LaravelCafebazaar": "Nikandlv\\LaravelCafebazaar\\LaravelCafebazaarFacade"
            }
        }
    }
}
